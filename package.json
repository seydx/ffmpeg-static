{
  "name": "@seydx/ffmpeg-static",
  "version": "0.0.1",
  "description": "FFmpeg static binaries for multiple platforms",
  "author": "seydx (https://github.com/seydx/camera.ui)",
  "main": "./dist/cli.js",
  "types": "./dist/cli.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/cli.js",
      "types": "./dist/cli.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write 'src/' --ignore-unknown --no-error-on-unmatched-pattern",
    "lint": "eslint --fix --ext .js,.ts .",
    "update": "updates --update ./",
    "install-updates": "npm i --save",
    "prepublishOnly": "npm i --package-lock-only && npm run lint && npm run format && npm run build"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "commander": "^12.1.0",
    "decompress": "^4.2.1",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-tarxz": "^3.0.0",
    "decompress-unzip": "^4.0.1",
    "fs-extra": "11.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/decompress": "^4.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "concurrently": "^9.0.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "updates": "^16.4.0"
  },
  "bugs": {
    "url": "https://github.com/seydx/camera.ui/issues"
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "homepage": "https://github.com/seydx/camera.ui#readme",
  "keywords": [
    "camera.ui",
    "ffmpeg"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seydx/camera.ui.git"
  }
}