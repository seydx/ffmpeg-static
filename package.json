{
  "name": "@seydx/ffmpeg-static",
  "version": "0.0.18",
  "description": "FFmpeg static binaries for multiple platforms",
  "author": "seydx (https://github.com/seydx/camera.ui)",
  "type": "module",
  "bin": {
    "ffmpeg-static": "dist/cli.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "format": "prettier --write 'src/' 'bin/' --ignore-unknown --no-error-on-unmatched-pattern",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "update": "updates --update ./",
    "install-updates": "npm i --save",
    "prepublishOnly": "npm i --package-lock-only && npm run lint && npm run format && npm run build"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "commander": "^14.0.0",
    "decompress": "^4.2.1",
    "decompress-tar": "^4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-tarxz": "^3.0.0",
    "decompress-unzip": "^4.0.1",
    "fs-extra": "11.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.12.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/decompress": "^4.2.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.0.12",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "concurrently": "^9.2.0",
    "eslint": "9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "updates": "^16.4.2"
  },
  "bugs": {
    "url": "https://github.com/seydx/camera.ui/issues"
  },
  "engines": {
    "node": ">=20.17.0"
  },
  "homepage": "https://github.com/seydx/camera.ui#readme",
  "keywords": [
    "camera.ui",
    "ffmpeg"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seydx/camera.ui.git"
  }
}
